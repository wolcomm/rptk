{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "query class result schema",
  "type" : "object",
  "properties": {
    "ipv4": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "prefix": {
            "type": "string",
            "format": "ipv4-prefix"
          },
          "exact": {"type": "boolean"},
          "less-equal": {
            "type": "integer",
            "minimum": 0,
            "maximum": 32
          },
          "greater-equal": {
            "type": "integer",
            "minimum": 0,
            "maximum": 32
          }
        },
        "required": ["prefix", "exact"],
        "additionalProperties": false
      },
      "uniqueItems": true
    },
    "ipv6": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "prefix": {
            "type": "string",
            "format": "ipv6-prefix"
          },
          "exact": {"type": "boolean"},
          "less-equal": {
            "type": "integer",
            "minimum": 0,
            "maximum": 128
          },
          "greater-equal": {
            "type": "integer",
            "minimum": 0,
            "maximum": 128
          }
        },
        "required": ["prefix", "exact"],
        "additionalProperties": false
      },
      "uniqueItems": true
    }
  },
  "required": ["ipv4", "ipv6"],
  "additionalProperties": false
}
